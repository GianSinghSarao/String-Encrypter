  <script>
    addEvent(window, "load", function (event) {
      var main = document.getElementById("PageContent");
      main.innerHTML += '\
          <div><span id="ZeroSize" aria-hidden="true" role="presentation">0</span></div>\
          <form id="form">\
            <div class="text">\
              <label id="StringInputLabel" for="StringInput">\
